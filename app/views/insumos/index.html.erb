<p id="notice"><%= notice %></p>

<h1>Insumos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Preco</th>
      <th>Descricao</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Icone</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insumos.each do |insumo| %>
      <tr>
        <td><%= insumo.tipo %></td>
        <td><%= insumo.preco %></td>
        <td><%= insumo.descricao %></td>
        <td><%= insumo.latitude %></td>
        <td><%= insumo.longitude %></td>
        <td><%= insumo.icone %></td>
        <td><%= insumo.foto %></td>
        <td><%= link_to 'Show', insumo %></td>
        <td><%= link_to 'Edit', edit_insumo_path(insumo) %></td>
        <td><%= link_to 'Destroy', insumo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
